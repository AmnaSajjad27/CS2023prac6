// Calculates y = |x|
// x & y are static variables

// if posiitve just spit it out 

push static 0
push constant 0

lt                     // check if x < 0
if-goto IF_TRUE
goto IF_FALSE

label IF_TRUE          // if x < 0, negate and return x
pop static 1        
return

// if negative, then negate and spit it out 

label IF_FALSE          // if x < 0, negate and return x
push static 0
neg
pop static 1        
return

label END
goto END